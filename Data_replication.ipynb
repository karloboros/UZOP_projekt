    "from sklearn.preprocessing import MinMaxScaler\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "medijani = ['Glucose','BloodPressure','SkinThickness','Insulin','BMI']\n",
    "standardizirane = ['Glucose','BloodPressure','Insulin','BMI', 'Age', 'DiabetesPedigreeFunction']\n",
    "\n",
    "for feature in medijani:\n",
    "    medijan = data[feature].median()\n",
    "    data[feature] = np.where(data[feature] == 0, medijan, data[feature])\n",
    "\n",
    "#scaler = MinMaxScaler()\n",
    "scaler = StandardScaler()\n",
    "data[standardizirane] = scaler.fit_transform(data[standardizirane])\n",
    "data[standardizirane] = round(data[standardizirane], 2)\n",
    "\n",
    "data.describe()"
